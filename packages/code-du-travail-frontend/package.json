{
  "name": "@cdt/frontend",
  "version": "4.67.1",
  "description": "Code du travail frontend",
  "private": true,
  "homepage": "https://code-du-travail.beta.gouv.fr/",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "dev": "node server/server.js",
    "lint-js": "eslint .{src,pages,public,test,server}/**/*.{js,ts,tsx,jsx} --no-error-on-unmatched-pattern",
    "lint-css": "stylelint 'src/**/*.{js,ts,tsx,jsx}' 'src/**/*.css' 'pages/**/*.{js,ts,tsx,jsx}'",
    "lint": "yarn lint-css && yarn lint-js",
    "precommit": "lint-staged",
    "prepush": "yarn test --bail --changedSince=master",
    "start": "NODE_ENV=production node server/server.js",
    "build": "next build",
    "analyze-bundles": "ANALYZE=true next build",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:update": "NODE_ENV=test jest -u",
    "generate-sitemap": "node -r esm ./scripts/generate-sitemap",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "@cdt/data": "^4.67.1",
    "@cdt/data...prime-precarite": "^4.67.1",
    "@cdt/data...simulateurs": "^4.67.1",
    "@cdt/data...tools": "^4.67.1",
    "@matejmazur/react-katex": "^3.1.3",
    "@next/bundle-analyzer": "^11.1.0",
    "@sentry/nextjs": "^6.13.3",
    "@socialgouv/cdtn-logger": "^1.8.10",
    "@socialgouv/cdtn-slugify": "^4.67.1",
    "@socialgouv/cdtn-sources": "^4.67.1",
    "@socialgouv/cdtn-ui": "^4.67.1",
    "@socialgouv/fiches-travail-data": "^4.241.0",
    "@socialgouv/modeles-social": "^4.67.1",
    "@socialgouv/react-fiche-service-public": "^4.67.1",
    "@socialgouv/reference-article": "^1.0.1",
    "@ungap/url-search-params": "^0.2.2",
    "@zeit/next-source-maps": "^0.0.3",
    "cheerio": "^1.0.0-rc.10",
    "date-fns": "^2.22.1",
    "debounce-promise": "^3.1.2",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "final-form-calculate": "^1.3.2",
    "katex": "^0.13.13",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-router": "^10.0.0",
    "lit-element": "3.0.2",
    "memoizee": "^0.4.15",
    "next": "^11.1.0",
    "next-transpile-modules": "^8.0.0",
    "p-debounce": "^2.1.0",
    "publicodes": "^1.0.0-beta.16",
    "react": "^17.0.2",
    "react-autosuggest": "10.1.0",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-final-form": "^6.5.3",
    "react-final-form-arrays": "^3.1.3",
    "react-final-form-listeners": "^1.0.3",
    "react-image-lightbox": "^5.1.4",
    "react-svg-spinner": "^1.0.4",
    "react-uid": "^2.3.1",
    "rehype-parse": "^8.0.3",
    "rehype-raw": "^5.1.0",
    "rehype-react": "^6.2.1",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.5",
    "unified": "^9.2.2",
    "uuid": "^8.3.2",
    "xss": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.15.0",
    "@socialgouv/eslint-config-react": "^1.75.0",
    "@testing-library/react": "^12.0.0",
    "@types/debounce-promise": "^3.1.4",
    "@types/memoizee": "^0.4.6",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^7.31.0",
    "esm": "^3.2.25",
    "ignore-loader": "^0.1.2",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^12.1.2",
    "mockdate": "^3.0.5",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2",
    "stylelint": "^13.13.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^6.1.6"
  },
  "lint-staged": {
    "{pages,src}/**/*.{css,js,ts,tsx,jsx}": [
      "yarn lint-css"
    ],
    "{__mocks__,test,__tests__,pages,server,src}/**/*.{js,ts,tsx,jsx}": [
      "yarn lint-js --fix"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/__tests__/**/*?(*.)+(test|spec).[jt]s?(x)"
    ],
    "collectCoverageFrom": [
      "!src/**/*mock.js",
      "src/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  }
}
