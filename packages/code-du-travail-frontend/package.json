{
  "name": "@cdt/frontend",
  "version": "3.7.0",
  "description": "Code du travail frontend",
  "private": true,
  "homepage": "https://code-du-travail.beta.gouv.fr/",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "dev": "node server/server.js",
    "dev-with-nlp": "node server/server.js",
    "lint-js": "eslint src/* pages/*",
    "lint-css": "stylelint 'src/**/*.js' 'src/**/*.css' 'pages/**/*.js'",
    "lint": "yarn lint-js && yarn lint-css",
    "prepush": "yarn lint && yarn test",
    "start": "NODE_ENV=production node server/server.js",
    "build": "next build",
    "test": "NODE_ENV=test jest",
    "generate-sitemap": "node -r esm ./scripts/generate-sitemap"
  },
  "dependencies": {
    "@cdt/data": "^3.7.0",
    "@cdt/data...datafiller": "^3.7.0",
    "@cdt/data...preavis-demission": "^3.7.0",
    "@cdt/data...preavis-licenciement": "^3.7.0",
    "@cdt/data...prime-precarite": "^3.7.0",
    "@cdt/sources": "^3.7.0",
    "@reach/tooltip": "^0.6.1",
    "@sentry/browser": "^5.9.1",
    "@socialgouv/react-fiche-service-public": "^3.7.0",
    "@socialgouv/react-ui": "^3.7.0",
    "@zeit/next-source-maps": "^0.0.3",
    "date-fns": "^2.8.1",
    "debounce-promise": "^3.1.2",
    "express": "^4.17.1",
    "express-sitemap-xml": "^1.1.0",
    "final-form": "^4.18.6",
    "final-form-arrays": "^3.0.2",
    "final-form-calculate": "^1.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "2.2.1",
    "mdx-runtime-slim": "^1.4.5",
    "memoizee": "^0.4.14",
    "next": "^9.1.4",
    "next-transpile-modules": "^2.3.1",
    "react": "^16.12.0",
    "react-autosuggest": "9.4.3",
    "react-delay": "^0.1.0",
    "react-dom": "^16.12.0",
    "react-feather": "2.0.3",
    "react-final-form": "^6.3.3",
    "react-final-form-arrays": "^3.1.1",
    "react-final-form-listeners": "^1.0.2",
    "react-mathjax-preview": "^0.1.16",
    "react-svg-spinner": "^1.0.4",
    "react-uid": "^2.2.0",
    "rehype-raw": "^4.0.1",
    "rehype-react": "^4.0.1",
    "remark-parse": "^7.0.2",
    "remark-rehype": "^5.0.0",
    "speakingurl": "14.0.1",
    "styled-components": "^4.4.1",
    "unified": "^8.4.2",
    "unist-util-find": "^1.0.1",
    "url-search-params": "^1.1.0",
    "use-persisted-state": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@socialgouv/eslint-config-react": "^0.10.3",
    "@testing-library/react": "^9.3.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.6.0",
    "esm": "^3.2.25",
    "ignore-loader": "^0.1.2",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "stylelint": "^12.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*?(*.)+(test|spec).[jt]s?(x)"
    ],
    "collectCoverageFrom": [
      "!src/**/*mock.js",
      "src/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  }
}
