{
  "name": "@cdt/frontend",
  "version": "4.10.0",
  "description": "Code du travail frontend",
  "private": true,
  "homepage": "https://code-du-travail.beta.gouv.fr/",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "dev": "node server/server.js",
    "lint-js": "eslint src/* pages/*",
    "lint-css": "stylelint 'src/**/*.js' 'src/**/*.css' 'pages/**/*.js'",
    "lint": "yarn lint-js && yarn lint-css",
    "prepush": "yarn lint && yarn test",
    "start": "NODE_ENV=production node server/server.js",
    "build": "next build",
    "analyze-bundles": "ANALYZE=true next build",
    "test": "NODE_ENV=test jest",
    "generate-sitemap": "node -r esm ./scripts/generate-sitemap"
  },
  "dependencies": {
    "@cdt/data": "^4.10.0",
    "@cdt/data...contributions": "^4.10.0",
    "@cdt/data...datafiller": "^4.10.0",
    "@cdt/data...prime-precarite": "^4.10.0",
    "@cdt/data...simulateurs": "^4.10.0",
    "@cdt/data...tools": "^4.10.0",
    "@cdt/sources": "^4.10.0",
    "@next/bundle-analyzer": "^9.3.1",
    "@reach/tooltip": "0.9.1",
    "@sentry/browser": "^5.15.0",
    "@sentry/node": "^5.15.0",
    "@socialgouv/react-fiche-service-public": "^4.10.0",
    "@socialgouv/react-ui": "^4.10.0",
    "@zeit/next-source-maps": "^0.0.3",
    "date-fns": "^2.11.0",
    "debounce-promise": "^3.1.2",
    "express-sitemap-xml": "^1.1.0",
    "final-form": "^4.18.7",
    "final-form-arrays": "^3.0.2",
    "final-form-calculate": "^1.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "2.2.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^5.2.0",
    "koa-router": "^8.0.8",
    "mdx-runtime-slim": "^1.4.5",
    "memoizee": "^0.4.14",
    "next": "9.3.1",
    "next-transpile-modules": "^3.1.0",
    "react": "^16.13.1",
    "react-autosuggest": "9.4.3",
    "react-burger-menu": "^2.6.13",
    "react-delay": "^0.1.0",
    "react-dom": "^16.13.1",
    "react-feather": "2.0.3",
    "react-final-form": "^6.3.5",
    "react-final-form-arrays": "^3.1.1",
    "react-final-form-listeners": "^1.0.2",
    "react-mathjax-preview": "^0.1.16",
    "react-svg-spinner": "^1.0.4",
    "react-uid": "^2.2.0",
    "rehype-raw": "^4.0.2",
    "rehype-react": "^5.0.0",
    "remark-parse": "^7.0.2",
    "remark-rehype": "^6.0.0",
    "speakingurl": "14.0.1",
    "styled-components": "^5.0.1",
    "tarteaucitronjs": "^1.3.1",
    "unified": "^8.4.2",
    "unist-util-find": "^1.0.1",
    "url-search-params": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@socialgouv/eslint-config-react": "^0.19.0",
    "@testing-library/react": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "esm": "^3.2.25",
    "ignore-loader": "^0.1.2",
    "intl": "^1.2.5",
    "jest": "^25.1.0",
    "jest-styled-components": "^7.0.0",
    "prettier": "^2.0.2",
    "prop-types": "^15.7.2",
    "stylelint": "^13.2.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.0.0",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*?(*.)+(test|spec).[jt]s?(x)"
    ],
    "collectCoverageFrom": [
      "!src/**/*mock.js",
      "src/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  }
}
