{
  "name": "@cdt/frontend",
  "version": "4.145.1",
  "description": "Code du travail frontend",
  "private": true,
  "homepage": "https://code-du-travail.beta.gouv.fr/",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "dev": "next dev",
    "lint:js": "next lint --quiet",
    "lint:css": "stylelint './{src,pages}/**/*.{js,ts,tsx,jsx,css}' --quiet",
    "lint": "yarn lint:css && next lint",
    "precommit": "lint-staged",
    "start": "next start",
    "prebuild": "node -r @swc-node/register scripts/prebuild.ts",
    "build": "yarn prebuild && next build",
    "build:analyze": "yarn prebuild && ANALYZE=true next build",
    "pretest:api": "NEXT_PUBLIC_ES_INDEX_PREFIX=cdtn_test NEXT_PUBLIC_APP_ENV=test NODE_ENV=test node -r @swc-node/register scripts/generate-data.ts",
    "test": "yarn pretest:api && NEXT_PUBLIC_ES_INDEX_PREFIX=cdtn_test NEXT_PUBLIC_APP_ENV=test NODE_ENV=test jest --silent",
    "test:watch": "npm run test -- --watch",
    "test:frontend": "NEXT_PUBLIC_APP_ENV=test NODE_ENV=test jest --silent --testPathIgnorePatterns=src/api",
    "test:frontend:update": "npm run test:frontend -- --updateSnapshot",
    "test:api": "npm run test -- --testPathPattern=src/api",
    "test:api:update": "npm run test:api -- --updateSnapshot",
    "test:update": "npm run test:frontend:update",
    "test:e2e": "cypress run",
    "test:e2e:ui": "cypress open",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.13.1",
    "@matejmazur/react-katex": "^3.1.3",
    "@next/bundle-analyzer": "^13.0.0",
    "@opentelemetry/instrumentation-generic-pool": "^0.37.0",
    "@opentelemetry/instrumentation-http": "^0.52.0",
    "@opentelemetry/instrumentation-net": "^0.37.0",
    "@sentry/nextjs": "^8.10.0",
    "@socialgouv/cdtn-elasticsearch": "^2.44.2",
    "@socialgouv/cdtn-logger": "^2.0.0",
    "@socialgouv/cdtn-types": "^2.42.3",
    "@socialgouv/cdtn-ui": "workspace:^",
    "@socialgouv/cdtn-utils": "workspace:^",
    "@socialgouv/fiches-travail-data": "^4.241.0",
    "@socialgouv/matomo-next": "^1.6.1",
    "@socialgouv/modeles-social": "workspace:^",
    "@socialgouv/reference-article": "^1.0.1",
    "@ungap/url-search-params": "^0.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "debounce-promise": "^3.1.2",
    "downshift": "^9.0.6",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "final-form-calculate": "^1.3.2",
    "fuzzball": "^2.1.2",
    "html-react-parser": "^5.0.6",
    "html-validate": "^8.9.1",
    "immer": "^9.0.14",
    "katex": "^0.16.10",
    "lit": "2.6.1",
    "lodash.deburr": "^4.1.0",
    "memoizee": "^0.4.15",
    "next": "14.2.4",
    "p-debounce": "^2.1.0",
    "prop-types": "^15.8.1",
    "publicodes": "1.0.0-beta.60",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.3",
    "react-final-form-arrays": "^3.1.3",
    "react-final-form-listeners": "^1.0.3",
    "react-image-lightbox": "^5.1.4",
    "react-svg-spinner": "^1.0.4",
    "react-uid": "^2.3.1",
    "sharp": "^0.33.4",
    "styled-components": "^5.3.6",
    "uuid": "^9.0.0",
    "xss": "^1.0.10",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.5",
    "@swc/core": "^1.3.60",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/cors": "^2.8.13",
    "@types/debounce-promise": "^3.1.4",
    "@types/jest": "^29.5.12",
    "cypress": "^13.9.0",
    "cypress-html-validate": "^6.1.0",
    "cypress-iframe": "^1.0.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-plugin-cypress": "^3.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.0.0",
    "mockdate": "^3.0.5",
    "nextjs-http-supertest": "^1.0.6",
    "postcss": "^8.4.19",
    "stylelint": "^14.16.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^6.3.3",
    "testing-library-selector": "0.3.1",
    "typescript": "^5.0.4"
  }
}
