{
  "name": "@cdt/frontend",
  "version": "4.91.1",
  "description": "Code du travail frontend",
  "private": true,
  "homepage": "https://code-du-travail.beta.gouv.fr/",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "dev": "next dev",
    "lint:js": "next lint --quiet",
    "lint:css": "stylelint './{src,pages}/**/*.{js,ts,tsx,jsx,css}' --quiet",
    "lint": "yarn lint:css && next lint",
    "precommit": "lint-staged",
    "start": "NODE_ENV=production next start",
    "prebuild": "node -r @swc-node/register scripts/prebuild.ts",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "pretest": "ES_INDEX_PREFIX=cdtn_test NODE_ENV=test node -r @swc-node/register scripts/generate-data.ts",
    "test": "ES_INDEX_PREFIX=cdtn_test NODE_ENV=test jest --silent",
    "test:frontend": "npm run test -- --testPathIgnorePatterns=src/api",
    "test:frontend:update": "npm run test:frontend -- --updateSnapshot",
    "test:api": "ES_INDEX_PREFIX=cdtn_test NODE_ENV=test npm run test -- --testPathPattern=src/api",
    "test:api:update": "ES_INDEX_PREFIX=cdtn_test NODE_ENV=test npm run test:api -- --updateSnapshot",
    "test:watch": "ES_INDEX_PREFIX=cdtn_test NODE_ENV=test jest --watch --silent",
    "test:update": "npm run test:frontend:update",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7",
    "@matejmazur/react-katex": "^3.1.3",
    "@next/bundle-analyzer": "^11.1.0",
    "@sentry/nextjs": "^7.36.0",
    "@socialgouv/cdtn-elasticsearch": "^2.5.2",
    "@socialgouv/cdtn-logger": "^2.0.0",
    "@socialgouv/cdtn-ui": "^4.91.1",
    "@socialgouv/cdtn-utils": "*",
    "@socialgouv/fiches-travail-data": "^4.241.0",
    "@socialgouv/matomo-next": "^1.6.1",
    "@socialgouv/modeles-social": "^4.91.1",
    "@socialgouv/reference-article": "^1.0.1",
    "@ungap/url-search-params": "^0.2.2",
    "date-fns": "^2.22.1",
    "debounce-promise": "^3.1.2",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "final-form-calculate": "^1.3.2",
    "immer": "^9.0.14",
    "katex": "^0.13.13",
    "lit": "2.6.1",
    "memoizee": "^0.4.15",
    "next": "^13.2.1",
    "p-debounce": "^2.1.0",
    "prop-types": "^15.8.1",
    "publicodes": "1.0.0-beta.60",
    "react": "^18.2.0",
    "react-autosuggest": "10.1.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.9",
    "react-final-form": "^6.5.3",
    "react-final-form-arrays": "^3.1.3",
    "react-final-form-listeners": "^1.0.3",
    "react-image-lightbox": "^5.1.4",
    "react-svg-spinner": "^1.0.4",
    "react-uid": "^2.3.1",
    "rehype-parse": "^8.0.3",
    "rehype-raw": "^5.1.0",
    "rehype-react": "^6.2.1",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.3.5",
    "unified": "^9.2.2",
    "uuid": "^9.0.0",
    "xss": "^1.0.10",
    "zustand": "^4.1.5"
