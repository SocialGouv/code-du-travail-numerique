{
  "name": "@cdt/frontend",
  "version": "2.3.1",
  "description": "Code du travail frontend",
  "private": true,
  "homepage": "https://code-du-travail.beta.gouv.fr/",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "dev": "node server.js",
    "lint": "eslint src/* pages/*",
    "prepush": "yarn lint && yarn test",
    "start": "NODE_ENV=production node server.js",
    "build": "next build",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@cdt/css": "^2.1.0",
    "@cdt/data": "^2.3.0",
    "@cdt/ui": "^2.3.0",
    "@sentry/browser": "^4.1.0",
    "@socialgouv/react-fiche-service-public": "^2.3.0",
    "@zeit/next-css": "^1.0.0",
    "date-fns": "^1.29.0",
    "fuse.js": "3.2.0",
    "isomorphic-unfetch": "^2.1.1",
    "koa": "^2.5.1",
    "koa-router": "^7.4.0",
    "memoizee": "^0.4.14",
    "next": "^8.0.3",
    "react": "^16.8.6",
    "react-autosuggest": "9.3.4",
    "react-delay": "^0.1.0",
    "react-dom": "^16.8.6",
    "react-feather": "1.1.0",
    "react-github-fork-ribbon": "0.5.1",
    "react-mathjax-preview": "^0.1.10",
    "react-piwik": "^1.6.0",
    "react-switch": "^3.0.4",
    "react-uid": "^2.0.1",
    "speakingurl": "14.0.1",
    "styled-components": "^4.2.0",
    "unist-util-find": "^1.0.1",
    "unist-util-parents": "^1.0.1",
    "url-search-params": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@socialgouv/eslint-config-react": "^0.5.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.13.0",
    "jest": "^24.5.0",
    "jest-styled-components": "^6.3.1",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      ".css$": "<rootDir>/test/stubCss.js"
    }
  }
}
