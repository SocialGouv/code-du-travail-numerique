{
  "plugins": ["cypress"],
  "extends": ["plugin:cypress/recommended", "next/core-web-vitals"],
  "parserOptions": {
    "project": "tsconfig.json",
    "sourceType": "module"
  },
  "rules": {
    "jest/valid-expect": "warn",
    "jest/expect-expect": "warn"
  },
  "env": {
    "cypress/globals": true
  }
}
